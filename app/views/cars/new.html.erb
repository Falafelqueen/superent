<div class=new-car-container>
  <h1>Create a new listing</h1>
  <br>
  <%= simple_form_for [@car], data:{turbo: false} do |f| %>
    <div>
    <h2>Car title</h2>
      <%= f.input :model%>
      <h2>When is your car available?</h2>
      <br>
      <h3>from</h3>
      <%= f.input :start_date %>
      <br>
      <h3>to</h3>
      <%= f.input :end_date %>
      <br>
      <h2>How much does the rental cost?</h2>
      <br>
      <%= f.input :price %>
      <%= f.submit "Create", class: "btn btn-primary" %>
    </div>
    <div>
    <h3>Description</h3>
      <%= f.input :description %>
    <h3>Add an image</h3>
      <%= f.input :image_url %>
    </div>
  <% end %>
</div>
